{
  "name": "pytv",
  "displayName": "PyTV",
  "description": "PyTV Language Support",
  "publisher": "Teddy-van-Jerry",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "author": {
		"name": "Wuqiong Zhao",
		"email": "me@wqzhao.org",
		"url": "https://wqzhao.org"
	},
  "categories": [
    "Programming Languages",
    "Debuggers"
  ],
  "activationEvents": [
    "onCommand:pytv.viewAuthor"
  ],
  "license": "MIT License",
  "repository": {
		"type": "git",
		"url": "https://github.com/autohdw/pytv-vscode"
	},
  "extensionDependencies": [
    "mshr-h.veriloghdl"
  ],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pytv.viewAuthor",
        "title": "PyTV Author"
      }
    ],
    "languages": [
      {
        "id": "pytv",
        "aliases": [
          "PyTV"
        ],
        "extensions": [
          ".pytv"
        ],
        "configuration": "./pytv-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pytv",
        "scopeName": "source.pytv",
        "path": "./syntaxes/pytv.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
